AUTHOR = 'krisr, bmahadev, gmoturu'
NAME = 'android_ACTS.WiFiEntRoaming'
TIME = 'MEDIUM'
TEST_TYPE = 'Server'
ATTRIBUTES = 'suite:android_wifi_ent_roaming'

DOC = """
This test runs Enterprise roaming tests against a WiFi test bed.

"""

def run(machine):
    job.run_test('android_ACTS',
                 testbed=hosts.create_testbed(machine),
                 config_file='wifi_ent_roaming.config',
                 test_file='wifi_ent_roaming',
                 acts_timeout=18000)

parallel_simple(run, machines)
